{
  "name": "lori",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:md",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "docs": "vuepress dev",
    "docs:build": "vuepress build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "generate-rest-clients": "bash ./generators/generateRestClients.sh && prettier --write \"src/api-clients/**/*.js\"",
    "getgorxy": "node scripts/downloadgorxy.js",
    "gorxy:darwin:linux": "./gorxy --port=8081 --prtgconfig=gorxyprtg.json || npm run getgorxy && npm run gorxy:darwin:linux",
    "gorxy:win32": ".\\gorxy --port=8081 --prtgconfig=gorxyprtg.json || npm run getgorxy && npm run gorxy:win32",
    "lint:eslint": "npx eslint --ext .js,.vue --fix .",
    "lint:md": "markdownlint -i \"{src/api-clients/**, node_modules/}\" docs/*.md *.md src/",
    "lint:prettier": "prettier --list-different --write \"**/*.{js,json,css,scss,vue,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.vue\" \"src/**/*.scss\"",
    "new": "hygen new",
    "start": "vue-cli-service serve",
    "storybook": "vue-cli-service serve:storybook -p 6006 -c .storybook",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watchAll",
    "vuelint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "iframe-resizer": "^3.6.3",
    "material-design-icons-iconfont": "^4.0.2",
    "register-service-worker": "^1.5.2",
    "resize-observer-polyfill": "^1.5.0",
    "superagent": "^4.0.0",
    "vee-validate": "^2.1.3",
    "vue": "^2.5.17",
    "vue-i18n": "^8.3.2",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.9",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@namics/stylelint-bem": "^5.1.1",
    "@storybook/addon-actions": "^4.1.0-alpha.8",
    "@storybook/addon-backgrounds": "^4.1.0-alpha.8",
    "@storybook/addon-knobs": "^4.1.0-alpha.8",
    "@storybook/addon-links": "^4.1.0-alpha.8",
    "@storybook/addon-notes": "^4.1.0-alpha.8",
    "@storybook/addons": "^4.0.9",
    "@storybook/vue": "^4.0.9",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-e2e-cypress": "^3.1.2",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-plugin-pwa": "^3.1.2",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-prettier": "4.0.1",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^3.0.10",
    "electron-devtools-installer": "^2.2.4",
    "hygen": "^1.6.2",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^8.1.0",
    "markdownlint-cli": "^0.13.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "stylelint": "9.8.x",
    "stylelint-config-css-modules": "1.3.x",
    "stylelint-config-prettier": "4.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.2.x",
    "stylelint-scss": "3.4.x",
    "vue-cli-plugin-electron-builder": "^1.0.0-rc.8",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=9.0.0",
    "yarn": ">=1.0.0"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "main": "background.js"
}
